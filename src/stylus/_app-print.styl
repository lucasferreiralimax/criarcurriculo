@media print
  *
    -ms-filternone !important
    background transparent !important
    box-shadow none !important
    filter none !important
    text-shadow none !important

  body
    &:before
      display none

  #curriculo,
  header,
  .header,
  .btn,
  .mark,
  .notice,
  .accounts,
  .layout,
  #final,
  .fontType,
  .themeApp
  .carousel
    display none !important
    opacity 0 !important

  .viewblocked
    opacity 1 !important

  .render, .view
    bottom 0 !important
    left 0 !important
    padding 0 !important
    right 0 !important
    top 0 !important
    width 100% !important
    &:after
      display none !important
    &.no-icon svg
      display none
    *
      display block
    svg
      display inline
    span, p, b
      display inline-block
    .content
      bottom 0 !important
      display block !important
      filter blur(0) !important
      font-weight normal
      height 100% !important
      left 0 !important
      overflow hidden !important
      padding 0 !important
      pointer-events none
      position relative !important
      right 0 !important
      top 0 !important
      transform scale(1) !important
      width 100% !important
    .layout-mirror .figure
      right 0
